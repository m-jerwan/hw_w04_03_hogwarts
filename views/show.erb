<h3>Student details:</h3>

<p>First name: <%= @student.first_name.capitalize%></p>
<p>Last name: <%= @student.last_name.capitalize%></p>
<p>House id: <%=@student.house_id%></p> <!-- FIX !!! -->
<p>Age: <%= @student.age%></p>

<form class="" action="/students/<%= @student.id %>/edit" method="post">
  <input type="submit" value="Edit">
</form>
</br>

<form class="" action="/students/<%= @student.id %>/delete" method="post">
  <input type="submit" value="DELETE">
</form>
